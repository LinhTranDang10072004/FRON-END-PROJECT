{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/taphoammo-fronend/browser",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
